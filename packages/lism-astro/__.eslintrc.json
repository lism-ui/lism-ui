{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"react/prop-types": 0,
		"react/no-unknown-property": [
			2,
			{
				"ignore": [
					"jsx",
					"global"
				]
			}
		]
	}
}
