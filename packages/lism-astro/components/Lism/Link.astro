---
import { LismProps } from '../types';
import getLinkProps from '@lism-ui/core/components/getLinkProps';

// Propsの定義
interface Props extends LismProps {}
const { as, exProps, ...props } = Astro.props || {};

const JSX = as || 'a';
---

<JSX {...getLinkProps(props)} {...exProps}>
	<slot />
</JSX>
