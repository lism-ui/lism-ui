---
import type { LismProps } from '../types';
import getLismProps from '../../../dist/lib/getLismProps';

// Propsの定義
interface Props extends LismProps {}

const { as, exProps, tag, ...props } = Astro.props || {};
const JSX = as || tag || 'div';
---

<JSX {...getLismProps(props)} {...exProps}>
	<slot />
</JSX>
